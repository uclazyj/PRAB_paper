@article{German2018,
	Abstract = {This article introduces an analytical formalism for the calculation of the evolution of beam moments and the transverse emittance for beams which are externally injected into plasma wakefield accelerators. This formalism is then applied to two scenarios with increasing complexity --- a single beam slice without energy gain and a single beam slice with energy gain, both propagating at a fixed co-moving position behind the driver. The obtained results are then compared to particle-in-cell (PIC) simulations as well as results obtained using an semi-analytic numerical approach (SANA) (Mehrling et al., 2016). We find excellent agreement between results from the analytical model and from SANA and PIC.},
	Author = {Alexander Aschikhin and Timon Johannes Mehrling and Alberto Martinez de la Ossa and Jens Osterhoff},
